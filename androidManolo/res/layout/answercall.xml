<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/widget64"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:orientation="vertical"
xmlns:android="http://schemas.android.com/apk/res/android"
android:background="@drawable/bbg"
android:layout_gravity="center_horizontal"
>
<LinearLayout
android:id="@+id/widget140"
android:layout_width="280px"
android:layout_height="350px"
android:orientation="vertical"
android:background="@drawable/emptybbg"
android:layout_gravity="center_horizontal"
>
<ImageView
android:id="@+id/contactimageview"
android:layout_width="250px"
android:layout_height="250px"
android:layout_marginTop="20px"
android:layout_gravity="center_horizontal"
android:src="@drawable/unknowncontact"

>
</ImageView>
<TextView
android:id="@+id/pushtcalltext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_marginTop="20px"
android:text="Ringing..."
android:layout_gravity="center_horizontal"
>
</TextView>
</LinearLayout>
<AbsoluteLayout
android:id="@+id/widget339"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_marginTop="30px"
android:layout_gravity="center_horizontal"
>
<ImageButton
android:id="@+id/hungcallimage"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/hungup_on_big"
android:layout_x="97px"
android:layout_y="-1px"
>
</ImageButton>
<ImageButton
android:id="@+id/docallimage"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/call_on_big"
android:layout_x="-1px"
android:layout_y="0px"
>
</ImageButton>
</AbsoluteLayout>
</LinearLayout>
